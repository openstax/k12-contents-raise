                                                    &lt;mn&gt;3&lt;/mn&gt;
                                                    &lt;mi&gt;x&lt;/mi&gt;
                                                    &lt;mo&gt;+&lt;/mo&gt;
                                                    &lt;mi&gt;y&lt;/mi&gt;
                                                    &lt;mo&gt;=&lt;/mo&gt;
                                                    &lt;mn&gt;12&lt;/mn&gt;
                                                &lt;/mtd&gt;
                                            &lt;/mtr&gt;
                                            &lt;mtr&gt;
                                                &lt;mtd columnalign="left"&gt;
                                                    &lt;mi&gt;x&lt;/mi&gt;
                                                    &lt;mo&gt;=&lt;/mo&gt;
                                                    &lt;mi&gt;y&lt;/mi&gt;
                                                    &lt;mo&gt;−&lt;/mo&gt;
                                                    &lt;mn&gt;8&lt;/mn&gt;
                                                &lt;/mtd&gt;
                                            &lt;/mtr&gt;
                                        &lt;/mtable&gt;
                                    &lt;/mrow&gt;
                                    &lt;mo&gt;.&lt;/mo&gt;
                                &lt;/mrow&gt;
                            &lt;/mrow&gt;
                            &lt;annotation-xml encoding="MathML-Content"&gt;
                                &lt;mrow&gt;
                                    &lt;mrow&gt;
                                        &lt;mo&gt;{&lt;/mo&gt;
                                        &lt;mtable&gt;
                                            &lt;mtr&gt;
                                                &lt;mtd columnalign="left"&gt;
                                                    &lt;mn&gt;3&lt;/mn&gt;
                                                    &lt;mi&gt;x&lt;/mi&gt;
                                                    &lt;mo&gt;+&lt;/mo&gt;
                                                    &lt;mi&gt;y&lt;/mi&gt;
                                                    &lt;mo&gt;=&lt;/mo&gt;
                                                    &lt;mn&gt;12&lt;/mn&gt;
                                                &lt;/mtd&gt;
                                            &lt;/mtr&gt;
                                            &lt;mtr&gt;
                                                &lt;mtd columnalign="left"&gt;
                                                    &lt;mi&gt;x&lt;/mi&gt;
                                                    &lt;mo&gt;=&lt;/mo&gt;
                                                    &lt;mi&gt;y&lt;/mi&gt;
